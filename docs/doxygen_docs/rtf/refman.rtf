86     {\cf17 static} {\cf18 char} envstr[100];\par
87     {\cf17 static} {\cf18 void} *instance = NULL;\par
88 \par
89     {\cf20 /* if we do not know the address of the singleton:}\par
90 {\cf20      * - maybe we are the one to create it (env variable also does not exist)}\par
91 {\cf20      * - or it is already created, and stored in the env variable. */}\par
92     {\cf19 if} (instance == NULL) \{\par
93         {\cf18 char} envvarname[100] = {\cf22 ""};\par
94         sprintf(envvarname, {\cf22 "%s%d"}, {\cf22 "debugmallocsingleton"}, ({\cf18 int}) getpid());\par
95         {\cf18 char} *envptr = getenv(envvarname);\par
96         {\cf19 if} (envptr == NULL) \{\par
97             {\cf20 /* no env variable: create singleton. */}\par
98             instance = debugmalloc_create();\par
99             sprintf(envstr, {\cf22 "%s=%p"}, envvarname, instance);\par
100             putenv(envstr);\par
101         \} {\cf19 else} \{\par
102             {\cf20 /* another copy of this function already created it. */}\par
103             {\cf18 int} ok = sscanf(envptr, {\cf22 "%p"}, &instance);\par
104             {\cf19 if} (ok != 1) \{\par
